    "cli",
    "es6",
    "es2015"
  ],
  "homepage": "https://github.com/axelpale/genversion",
  "main": "index.js",
  "bin": {
    "genversion": "bin/genversion.js"
  },
  "author": {
    "name": "Akseli Palen",
    "email": "akseli.palen@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/axelpale/genversion.git"
  },
  "license": "MIT",
  "dependencies": {
    "commander": "^2.11.0",
    "find-package": "^1.0.0",
    "firstline": "^1.2.1",
    "mkdirp": "^0.5.1"
  },
  "devDependencies": {
    "command-line-test": "^1.0.10",
    "eslint": "^5.11.1",
